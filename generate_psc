#!/bin/fish
perl -e '$_=join("",<>);s%/\*.*?\*/%%gs;s%//.*$%%gm;print' | sed -r "s/^(.*? )(\()(.*?)(\))( \{)/\1\3\5/g" | sed -r "s/(import)(.*)//g" | sed -e "s/System.out.//g" | sed -r "s/(.*)(throws )(.*\ )(.*)/\1\4/g" | sed -r "s/<.*>//g" | sed -e "s/ : / in /g" | sed -r "s/(.*)( \()(.*)(\) \{)/\1\3\4/g" | sed -e's/[[:space:]]*$//' | sed -e 's/;$//g' | sed -e 's/ {/:/g' | sed -e "s/}//g" | sed -e "s/\t /\t/g" | sed -r '/^\s*$/d'
